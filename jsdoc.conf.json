{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [
      "./"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "exclude": [
      ".git",
      ".idea",
      ".vscode",
      "node_modules"
    ]
  },
  "plugins": [],
  "opts": {
    "encoding": "utf8",
    "recurse": true
  }
}
